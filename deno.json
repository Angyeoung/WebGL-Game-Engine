{
    "tasks": {
        "build": "deno run -A build.ts",
        "start": "deno run src/main"
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@1"
    },
    "compilerOptions": {
        "lib": ["deno.ns", "dom"]
    },
    "fmt": {
        "options": {
            "useTabs": false,
            "indentWidth": 4,
            "semiColons": true,
            "singleQuote": true
        }
    }
}
